# Environment Variables Template
# Copy this file to .env and update with your actual values

# React App Configuration
REACT_APP_ENV=production
REACT_APP_API_KEY=your-google-gemini-api-key-here
REACT_APP_API_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-2.5-flash-preview-05-20:generateContent
REACT_APP_SITE_URL=https://your-domain.com

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false

# Performance Optimization
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_CACHE_DURATION=3600

# Security
REACT_APP_SECURE_MODE=true

# Deployment
BUILD_PATH=build
PUBLIC_URL=/

# Optional: Custom Port (if needed)
# PORT=3000